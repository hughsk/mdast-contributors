{
  "name": "remark-contributors",
  "version": "3.1.0",
  "description": "remark plugin to inject a given list of contributors into a table in a markdown file",
  "license": "MIT",
  "keywords": [
    "text",
    "markdown",
    "contributors",
    "contributor",
    "remark",
    "generation",
    "inject",
    "automatic"
  ],
  "repository": "hughsk/remark-contributors",
  "bugs": "https://github.com/hughsk/remark-contributors/issues",
  "author": "Hugh Kennedy <hughskennedy@gmail.com> (https://hughsk.io)",
  "contributors": [
    "Hugh Kennedy <hughskennedy@gmail.com> (https://hughsk.io)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Vincent Weevers <mail@vincentweevers.nl> (https://vincentweevers.nl)",
    "Nick Baugh <niftylettuce@gmail.com> (https://niftylettuce.com)"
  ],
  "files": [
    "headers.js",
    "index.js"
  ],
  "dependencies": {
    "is-url": "^1.2.2",
    "mdast-util-to-string": "^1.0.0",
    "parse-author": "^2.0.0"
  },
  "devDependencies": {
    "diff": "^3.5.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "remark": "^10.0.1",
    "remark-cli": "^6.0.1",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.9.1",
    "xo": "^0.23.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      "."
    ]
  }
}
