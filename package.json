{
  "name": "remark-contributors",
  "version": "6.0.2",
  "description": "remark plugin to inject a given list of contributors into a table",
  "license": "MIT",
  "keywords": [
    "automatic",
    "contributor",
    "contributors",
    "generation",
    "inject",
    "markdown",
    "mdast",
    "plugin",
    "remark",
    "remark-plugin",
    "unified"
  ],
  "repository": "remarkjs/remark-contributors",
  "bugs": "https://github.com/remarkjs/remark-contributors/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Hugh Kennedy <hughskennedy@gmail.com> (https://hughsk.io)",
  "contributors": [
    "Hugh Kennedy <hughskennedy@gmail.com> (https://hughsk.io)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Vincent Weevers <mail@vincentweevers.nl> (https://vincentweevers.nl)",
    "Nick Baugh <niftylettuce@gmail.com> (https://niftylettuce.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": {
    "./lib/get-contributors-from-package.js": "./lib/get-contributors-from-package.browser.js"
  },
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/mdast": "^4.0.0",
    "is-url": "^1.0.0",
    "mdast-util-heading-range": "^4.0.0",
    "parse-author": "^2.0.0",
    "type-fest": "^4.0.0",
    "vfile": "^6.0.0",
    "vfile-find-up": "^7.0.0"
  },
  "devDependencies": {
    "@types/is-url": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/parse-author": "^2.0.0",
    "c8": "^8.0.0",
    "prettier": "^3.0.0",
    "remark": "^15.0.0",
    "remark-cli": "^11.0.0",
    "remark-gfm": "^4.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "to-vfile": "^8.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^5.0.0",
    "xo": "^0.56.0"
  },
  "scripts": {
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . --frail --output --quiet && prettier . --log-level warn --write && xo --fix",
    "prepack": "npm run build && npm run format",
    "test": "npm run build && npm run format && npm run test-coverage",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --100 --reporter lcov npm run test-api"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm",
      "./index.js"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreCatch": true,
    "strict": true
  },
  "xo": {
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "no-await-in-loop": "off"
        }
      }
    ],
    "prettier": true,
    "rules": {
      "unicorn/prefer-string-replace-all": "off"
    }
  }
}
